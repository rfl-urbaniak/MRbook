// Modified Murder Case DAG with Other's Fingerprints
digraph {
	"father/son" [label="father/son"]
	confession [label=confession]
	angle [label=angle]
	father_fingerprints [label="father's fingerprints"]
	sons_fingerprints [label="son's fingerprints"]
	"father/son" -> father_fingerprints
	"father/son" -> sons_fingerprints
	"father/son" -> confession
	"father/son" -> angle
}
