// Murder Case DAG
digraph {
	F [label=Father]
	C [label=Confession]
	A [label=Angle]
	FF [label="Father Fingerprints"]
	P [label="Posterior Father No Fingerprints"]
	F -> C [label="Influences Confession"]
	F -> A [label="Influences Angle"]
	F -> FF [label="Influences Fingerprints"]
	C -> P
	A -> P
	FF -> P
}
