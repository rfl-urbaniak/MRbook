// Modified Murder Case DAG
digraph {
	father [label=Father]
	confession [label=Confession]
	angle [label=Angle]
	father_fingerprints [label="Father Fingerprints"]
	posterior_father_fathers_fingerprint [label="Posterior Father Father's Fingerprints"]
	posterior_father_not_fathers_fingerprint [label="Posterior Father Not Father's Fingerprints"]
	posterior_father_no_fingerprint [label="Posterior Father No Fingerprint"]
	father -> father_fingerprints
	father -> confession
	father -> angle
	father_fingerprints -> posterior_father_fathers_fingerprint
	father_fingerprints -> posterior_father_not_fathers_fingerprint
	confession -> posterior_father_fathers_fingerprint
	confession -> posterior_father_not_fathers_fingerprint
	confession -> posterior_father_no_fingerprint
	angle -> posterior_father_fathers_fingerprint
	angle -> posterior_father_not_fathers_fingerprint
	angle -> posterior_father_no_fingerprint
}
