// Modified Murder Case DAG with Other's Fingerprints
digraph {
	father [label=Father]
	confession [label=Confession]
	angle [label=Angle]
	father_fingerprints [label="Father's Fingerprints"]
	others_fingerprints [label="Other's Fingerprints"]
	posterior_father_fathers_fingerprint [label="Posterior Father's Fingerprints"]
	posterior_father_not_fathers_fingerprint [label="Posterior Other's Fingerprints"]
	posterior_father_no_fingerprint [label="Posterior No Fingerprints"]
	father -> father_fingerprints
	father -> others_fingerprints
	father -> confession
	father -> angle
	father_fingerprints -> posterior_father_fathers_fingerprint
	father_fingerprints -> posterior_father_not_fathers_fingerprint
	others_fingerprints -> posterior_father_not_fathers_fingerprint
	others_fingerprints -> posterior_father_fathers_fingerprint
	confession -> posterior_father_fathers_fingerprint
	confession -> posterior_father_not_fathers_fingerprint
	confession -> posterior_father_no_fingerprint
	angle -> posterior_father_fathers_fingerprint
	angle -> posterior_father_not_fathers_fingerprint
	angle -> posterior_father_no_fingerprint
}
